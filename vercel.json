{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" }
  ],
  "builds": [
    {
      "src": "agraiBasedRagSever/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "agribasedRag/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ]
}
